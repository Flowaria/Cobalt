<Window x:Class="Cobalt.Windows.PropertiesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cobalt.Windows"
        mc:Ignorable="d"
        Title="설정" Height="600" Width="800" ResizeMode="NoResize"
        Closing="Window_Closing">
    <Grid>
        <TabControl x:Name="tabControl" Height="600" Width="800" Style="{StaticResource TabControl_Liner}">
            <TabItem Header="프로그램 설정" Foreground="black" Width="200">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="생성 정렬 형식" Foreground="black" Width="200">

            </TabItem>
        </TabControl>
    </Grid>
</Window>
